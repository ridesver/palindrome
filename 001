#include <iostream>
#include <string>


using namespace std;

bool palindrome(string word)
{
	int Length = word.length();
		for (int i = 0; i < Length / 2; i++)
		if (word[i] != word[Length - i - 1]) 
		return false;
		else return true;
}
int main()
{
	string slovo;
	cout << "Enter your word: ";
	cin >> slovo;
	if (palindrome(slovo)) 
	cout << true;
	else cout << false;
	return 0;
}
